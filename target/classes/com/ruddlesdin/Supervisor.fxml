<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="300.0" prefWidth="300.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <Label layoutX="105.0" layoutY="14.0" text="Supervisor">
            <font>
                <Font name="System Bold" size="18.0" />
            </font>
        </Label>
        <PasswordField fx:id="txtSupervisorPassword" layoutX="15.0" layoutY="174.0" onKeyReleased="#checkPwdText" promptText="Password" />
        <Label layoutX="16.0" layoutY="150.0" text="Password" />
        <Button fx:id="btnSupervisorClose" layoutX="209.0" layoutY="261.0" mnemonicParsing="false" onAction="#closeSupervisor" prefHeight="25.0" prefWidth="78.0" text="Close" />
        <Button fx:id="btnSupervisorUnlock" layoutX="220.0" layoutY="174.0" mnemonicParsing="false" onAction="#testPassword" text="Unlock" />
        <Label layoutX="34.0" layoutY="59.0" text="This action is locked !" />
        <Label layoutX="32.0" layoutY="126.0" text="Enter the supervisor password below." />
        <TextField fx:id="txtSupervisorMessage" alignment="CENTER" editable="false" focusTraversable="false" layoutX="76.0" layoutY="85.0" text="Locked !" />
        <TextField fx:id="txtSucceedMessage" editable="false" layoutX="14.0" layoutY="225.0" prefHeight="25.0" prefWidth="273.0" />
    </children>
</AnchorPane>
